{
  "compilerOptions": {
    "target": "ES6",
    
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*.ts" ,
              "src/Types/**/*"],
  "exclude": ["node_modules"]
}
